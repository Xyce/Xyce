    `tempdep(ETA0_t, ETA0_i, TETA0)
    if (CVMOD == 1) begin
        `tempdep(ETA0CV_t, ETA0CV_i, TETA0CV)
    end
    `tempdep(ETA0R_t, ETA0R_i, TETA0R)
    `tempdep(ETAMOB_t, ETAMOB_i, EMOBT_i)
    `tempdep(RSDR_t, RSDR, TRSDR)
    if (ASYMMOD != 0) begin
