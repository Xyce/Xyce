        rdstemp = `hypsmooth((T2 - 1.0e-6), 1.0e-3);
        T4 = `smoothmaxx2(Tnom, 210, 0.2);
        dvth_temp = (KT1_i + KT1L / Leff) * TRatio_m1 + KT11 / (1 + `lexp(KT12 * (DevTemp1 - TVTH))) - KT11 / (1 + `lexp(KT12 * (T4 - TVTH)));
    end
    if (VSAT_t < 1000) begin
