        end

        pParam_B4SOIleffCVbg = pParam_B4SOIleffCVb + 2 * B4SOIdlbg;
        if (pParam_B4SOIleffCVbg <= 0.0) begin
            $strobe("Effective channel length for C-V (backgate) <= 0");
            $finish(0);
        end

        /* Not binned - START */
