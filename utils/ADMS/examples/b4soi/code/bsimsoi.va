// Clamped log function
analog function real lln;
    input x;
    real x;
    begin
        lln = (x>`N_MINLOG) ? ln(x) : `ln_N_MINLOG;
    end
endfunction

// Hypsmooth function
analog function real hypsmooth;

       input x,c;
       real x, c;
       begin
            hypsmooth = 0.5*(x +  sqrt(x*x+4.0*c*c ));
       end

endfunction
