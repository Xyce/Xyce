# This is a skeletal CPackConfig.cmake that can be "configured" by 
# autoconf so we can do CPack native installers without CMake builds.

# Stuff like @CPACK_GENERATOR@, @XYCE_SRCDIR@, etc, are autoconf substitutions.
#
# INSTALLERSCRIPT needs to be replaced right before the nightly
# test scripts try to cpack on this deal.  That's because the actual path to
# the installer script is chosen by the build/test scripts themselves, not
# at configure/build time.

SET(CPACK_RPM_PACKAGE_DEBUG "ON")
SET(CPACK_CMAKE_GENERATOR "Unix Makefiles")
SET(CPACK_GENERATOR "@CPACK_GENERATOR@")
SET(CPACK_INSTALL_PREFIX "/usr/local")
SET(CPACK_NSIS_DISPLAY_NAME "CMake 2.5")
SET(CPACK_OUTPUT_CONFIG_FILE "@XYCE_BUILDDIR@/CPackConfig_out.cmake")
SET(CPACK_SET_DESTDIR "OFF")
SET(CPACK_PACKAGE_RELOCATABLE "true")
SET(CPACK_PACKAGE_DEFAULT_LOCATION "/usr/local")
SET(CPACK_PACKAGE_DESCRIPTION_FILE "@XYCE_SRCDIR@/distribution/CPack.Description.txt")
SET(CPACK_PACKAGE_DESCRIPTION_SUMMARY "Xyce Parallel Electronic Simulator")
SET(CPACK_PACKAGE_EXECUTABLES "Xyce;Xyce")
SET(CPACK_PACKAGE_FILE_NAME "@CPACK_PACKAGE_FILE_NAME@")
SET(CPACK_PACKAGE_INSTALL_REGISTRY_KEY "Xyce")
SET(CPACK_PACKAGE_NAME "@CPACK_PACKAGE_NAME@")
SET(CPACK_PACKAGE_VENDOR "Sandia National Laboratories")
SET(CPACK_PACKAGE_VERSION "@Xyce_VERSION@")
SET(CPACK_PACKAGE_VERSION_MAJOR "@Xyce_VERSION_MAJOR@")
SET(CPACK_PACKAGE_VERSION_MINOR "@Xyce_VERSION_MINOR@")
SET(CPACK_PACKAGE_VERSION_PATCH "@Xyce_VERSION_PATCH@")
SET(CPACK_PACKAGING_INSTALL_PREFIX @CPACK_PACKAGING_INSTALL_PREFIX@)
SET(CPACK_PACKAGE_INSTALL_DIRECTORY "@CPACK_PACKAGE_NAME@")
SET(CPACK_RESOURCE_FILE_LICENSE "@XYCE_SRCDIR@/distribution/CPack.@XYCE_LICENSE@License.txt")
SET(CPACK_RESOURCE_FILE_README "@XYCE_SRCDIR@/distribution/CPack.Description.txt")
SET(CPACK_RESOURCE_FILE_WELCOME "@XYCE_SRCDIR@/distribution/CPack.Welcome.txt")
SET(CPACK_SOURCE_GENERATOR "TGZ;TZ")
SET(CPACK_SOURCE_OUTPUT_CONFIG_FILE "@XYCE_BUILDDIR@/CPackSourceConfig.cmake")
SET(CPACK_SOURCE_PACKAGE_FILE_NAME "Xyce-6.0.0")
SET(CPACK_SOURCE_STRIP_FILES "")
SET(CPACK_STRIP_FILES "bin/Xyce")
SET(CPACK_SYSTEM_NAME "@CPACK_SYSTEM_NAME@")
SET(CPACK_TOPLEVEL_TAG "@CPACK_SYSTEM_NAME@")
SET(CPACK_RPM_PACKAGE_DESCRIPTION "For more information, visit http://xyce.sandia.gov .")
SET(CPACK_RPM_PACKAGE_GROUP "Applications/Engineering")
SET(CPACK_RPM_PACKAGE_LICENSE "@CPACK_RPM_PACKAGE_LICENSE@")
SET(CPACK_RPM_PACKAGE_NAME @CPACK_RPM_PACKAGE_NAME@)
SET(CPACK_RPM_EXCLUDE_FROM_AUTO_FILELIST_ADDITION "/usr/local")
@CPACK_RPM_DEPENDS_SET_LINE@
SET(CPACK_INSTALL_COMMANDS "INSTALLERSCRIPT _CPack_Packages/${CPACK_TOPLEVEL_TAG}/${CPACK_GENERATOR}/${CPACK_PACKAGE_FILE_NAME}/${CPACK_PACKAGING_INSTALL_PREFIX}/${CPACK_PACKAGE_INSTALL_DIRECTORY}")

