          newExp.getMasterNodeVec().push_back(log10Ptr);
          newExp.getMasterNodeVec().push_back(returnPtr);
        }
      }
      else
      {
        std::cout << "Unrecognized internal variable operator " << firstArg << std::endl;
      }

      $$ = returnPtr;
      delete internalDevVarPtr;
      delete $1;
      delete $2;
    }
